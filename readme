npm i -g @nestjs/cli
nodejs>=10.13
node --version
nest new nestjs-project
cd nestjs-project
npm run start
localhost:3000
npm run start:dev
//for generate controller
nest g controller users
// for generate module
nest generate module employee
nest g controller employee/roles
//provider
nest g service blogs
*** third party module called (swagger)
npm i class-validator
npm install class-transformer

npm i -D @types/multer
npm i --save @nestjs/typeorm typeorm@0.2 mysql2

npm i --save @nestjs/config
nest g resource demo super-admin
